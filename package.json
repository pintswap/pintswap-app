        "stream-browserify": "^3.0.0",
        "stream-http": "^3.2.0",
        "string_decoder": "^1.3.0",
        "tailwindcss": "^3.2.7",
        "timers-browserify": "^2.0.12",
        "tls-browserify": "^0.2.2",
        "tty-browserify": "^0.0.1",
        "url": "^0.11.0",
        "util": "^0.12.5",
        "vm-browserify": "^1.1.2",
        "ws-browserify": "^1.0.0"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --quiet --fix"
        ],
        "*.{json,js,ts,jsx,tsx,html}": [
            "prettier --check --write"
        ]
    }
