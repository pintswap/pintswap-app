{
    "name": "pintswap-app",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "yarn build:css && react-app-rewired start",
        "start:dev": "REACT_APP_NETWORK=LOCALHOST GENERATE_SOURCEMAP=false BROWSER=none yarn start",
        "build": "yarn build:css && react-app-rewired build",
        "test": "jest",
        "eject": "react-app-rewired eject",
        "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
        "lint": "eslint --fix",
        "pretty": "prettier --write"
    },
    "dependencies": {
        "@headlessui/react": "^1.7.13",
        "@rainbow-me/rainbowkit": "^0.12.2",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/node": "^18.15.5",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/react-icons": "^3.0.0",
        "@types/websocket": "^1.0.5",
        "ethers": "^5.7.2",
        "lodash.merge": "^4.6.2",
        "pintswap-sdk": "git+https://github.com/pintswap/pintswap-sdk",
        "react": "^18.2.0",
        "react-copy-to-clipboard": "^5.1.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.8.0",
        "react-router-dom": "^6.9.0",
